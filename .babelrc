{
  "presets": [
    "env"
  ],
  "plugins": [
    [
      "babel-plugin-transform-builtin-extend",
      {
        "globals": [
          "Error"
        ]
      }
    ]
  ],
  "env": {
    "coverage": {
      "plugins": [
        [
          "istanbul",
          {
            "exclude": [
              "lib",
              "test"
            ]
          }
        ]
      ]
    }
  }
}
